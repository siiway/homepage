# 邮箱重定向 / Email Redirects
/t/m/:name /go/mail.html?name=:name
/t/m/:name/:domain /go/mail.html?name=:name&domain=:domain

# 路径别名 / Path Aliases
/a/:path /about/:path
/m/:name /members/:name
/member/:name /members/:name